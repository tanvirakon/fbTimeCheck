{
  "manifest_version": 3,
  "name": "Facebook Time Tracker",
  "version": "1.0",
  "permissions": ["tabs", "alarms", "storage", "notifications"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon1.png"
    }
  }
}
